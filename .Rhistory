fixDigWild <- function(enzx, enzFIX, sequence, coord1)
{
diff_array <- check_diff(enzx, enzFIX)
for(k in diff_array)
{
fix_coord <- coord1 + k - 1
substr(sequence, fix_coord, fix_coord) <- substr(enzFIX, k, k)
}
return(sequence)
}
